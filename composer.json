{
  "name": "minvws/laravel-crypto",
  "description": "Laravel provider for crypto functionality",
  "keywords": [
    "laravel",
    "security",
    "crypto",
    "cryptography",
    "signature"
  ],
  "type": "library",
  "license": "EUPL",
  "authors": [
    {
      "name": "Anne Jan Brouwer",
      "email": "brouwer@annejan.com"
    },
    {
      "name": "Nick ten Cate",
      "email": "nick@nicktencate.com"
    },
    {
      "name": "Mendel Mobach",
      "email": "mendel@mobach.nl"
    },
    {
      "name": "Joshua Thijssen",
      "email": "jthijssen@noxlogic.nl"
    }
  ],
  "require": {
    "php": "^7.3|^7.4|^8.0",
    "laravel/framework": "^6.20.17|^7.30.4|^8.31.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "^4.4",
    "phpstan/phpstan": "^0.12.80",
    "squizlabs/php_codesniffer": "^3.6",
    "mockery/mockery": "^1.4.3"
  },
  "autoload": {
    "psr-4": {
      "MinVWS\\Crypto\\Laravel\\": "src/"
    },
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MinVWS\\Crypto\\Laravel\\Tests\\": "tests/"
    }
  },
  "suggest": {
    "ext-openssl": "Install the OpenSSL extension in order to speed up a wide variety of cryptographic operations."
  },
  "extra": {
    "laravel": {
      "providers": [
        "MinVWS\\Crypto\\Laravel\\CryptoServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": [
        "vendor/bin/phpunit",
        "vendor/bin/phpstan analyse",
        "vendor/bin/psalm",
        "vendor/bin/phpcs"
    ]
  }
}
